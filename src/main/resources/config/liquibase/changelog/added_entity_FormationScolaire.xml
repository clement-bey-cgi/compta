<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>
    
    <!--
        Added the entity FormationScolaire.
    -->
    <changeSet id="formation_scolaire_table_changeset" author="jhipster">
        <createTable tableName="formation_scolaire">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="obtention_diplome" type="date">
                <constraints nullable="true" />
            </column>

            <column name="niveau_de_diplome" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="institut_de_formation" type="varchar(255)">
                <constraints nullable="true" />
            </column>            
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
    </changeSet>
    <changeSet id="formation_scolaire-1-data" author="jhipster" context="faker">
        <loadData
                  file="config/liquibase/fake-data/formation_scolaire.csv"
                  separator=";"
                  tableName="formation_scolaire">
            <column name="id" type="numeric"/>
            <column name="obtention_diplome" type="date"/>
            <column name="niveau_de_diplome" type="string"/>
			<column name="institut_de_formation" type="string"/>

            <!-- jhipster-needle-liquibase-add-loadcolumn - JHipster (and/or extensions) can add load columns here, do not remove-->
        </loadData>
    </changeSet>
</databaseChangeLog>
